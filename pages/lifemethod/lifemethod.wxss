  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}

.life_main_box_list_li_box_text {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  border: 1px solid #ccc;
  box-sizing: border-box;
  border-radius: 50%;
  text-align: center;
  line-height: 60rpx;
}

.color1 {
  color: #ec6d56;
}

.color2 {
  color: #6c45d5;
