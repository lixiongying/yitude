.life_bottom_btn {
  position: relative;
  width: 300rpx;
  height: 128rpx;
  margin: 50rpx auto;
}

.life_bottom_btn_btn {
  position: absolute;
  left: 0;
  top: 0;
  width: 300rpx;
  height: 128rpx;
  background-color: #6c45d5;
  box-shadow: 0px 8rpx 20rpx 0px rgba(135, 100, 233, 0.75);
  border-radius: 63rpx;
