.container {
  background: #f0f0f0;
  padding-top: 25rpx;
}
.address_box {
  width: 700rpx;
  margin: 0 auto;
}
.addku_li {
  width: 100%;
  height: 102rpx;
  margin: 0 auto;
  line-height: 102rpx;
  background: #ffffff;
  border-bottom: 1px solid #f0f0f0;
}
.addku_li_name {
  width: 130rpx;
  float: left;
  font-size: 30rpx;
  color: #202020;
  margin-left: 25rpx;
}
.addku_li_ipt {
  float: left;
  font-size: 30rpx;
  color: #202020;
  width: 440rpx;
  height: 102rpx;
}
.addku_li_ipt input {
  padding: 0 20rpx;
  margin: 0;
  width: 100%;
  height: 100%;
}
.addku_li_next {
  width: 16rpx;
  height: 30rpx;
  float: right;
  margin-top: 32rpx;
  margin-right: 25rpx;
}
.addku_li_next image {
  width: 100%;
  height: 100%;
  float: left;
}
.address {
  width: 702rpx;
	height: 88rpx;
	background-color: #ec6d56;
	border-radius: 10rpx;
  font-family: PingFang-SC-Medium;
	font-size: 32rpx;
	line-height: 88rpx;
	letter-spacing: 0px;
	color: #ffffff;
  text-align: center;
  margin-left: 24rpx;
  margin-top: 510rpx;
}
.input_class{
  color: #cccccc;
}