.result{
  width: 750rpx;
	height: 132rpx;
	background-color: #ffffff;
  border-bottom: 1px solid #f5f5f5
}
.result:last-child{
  border-bottom: none;
}
.result .time{
  max-width: 650rpx;
	height: 132rpx;
  line-height: 132rpx;
  font-size: 32rpx;
  color: #1f1f1f;
	background-color: #ffffff;
  padding-left: 24rpx;
  float: left;
}
.result .icon{
  width: 18rpx;
	height: 30rpx;
  float: right;
  margin-top: 51rpx;
  margin-right: 24rpx;
}
.msg {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  color: #787878;
  text-align: center;
}