.container{
  width: 100%;
  height: auto;
  
}
.container .main{
  width: 100%;
  height: auto;
  margin-bottom: 20rpx;
}
.main .main_head{
  width: 750rpx;
	height: 80rpx;
	background-color: #ffffff;
}
.main .main_head .head_img{
  width: 48rpx;
	height: 48rpx;
  float: left;
  margin-left: 24rpx;
  margin-top: 16rpx;
}
.main .head_title{
  max-width: 600rpx;
  height: 80rpx;
  text-align: left;
  padding-left: 24rpx;
  line-height: 80rpx;
  font-size: 26rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #1f1f1f;
  float: left;
}
.main .head_state{
  height: 80rpx;
  line-height: 80rpx;
  font-family: PingFang-SC-Medium;
	font-size: 26rpx;
	font-weight: normal;
	font-stretch: normal;
	float: right;
	letter-spacing: 0px;
	color: #ffa000;
  margin-right: 24rpx;
}
.main_ul{
