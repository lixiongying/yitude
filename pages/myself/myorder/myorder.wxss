}
.main_ul .f_ul{
  width: 750rpx;
	height: 210rpx;
	background-color: #ffffff
}
.main_ul .f_ul .number{
  width: 60rpx;
	height: 36rpx;
  float: right;
  margin-right: 25rpx;
  margin-top: 135rpx;
}

.main_ul .goods_img{
  width: 148rpx;
	height: 148rpx;
  float: left;
  margin-left: 24rpx;
  margin-top: 31rpx;
}
.main_ul .main_item{
  max-width: 550rpx;
  height: 100%;
  float: left;
}
.main_ul .main_item{
  max-width: 550rpx;
  height: 100%;
  margin-left: 20rpx;
}
.main_ul .main_item .item_one{
  max-width: 550rpx;
  height:105rpx;
  line-height: 105rpx;
  font-family: PingFang-SC-Medium;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #1f1f1f;
}
.main_ul .main_item .item_two{
  max-width: 550rpx;
  height:105rpx;
  line-height: 105rpx;
  font-family: PingFang-SC-Medium;
	font-size: 36rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #ec6d56;
}
.main_price{
  width: 100%;
  height: 80rpx;
	background-color: #ffffff;
  text-align: right;
  font-family: PingFang-SC-Medium;
	font-size: 26rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 80rpx;
	letter-spacing: 0px;
	color: #1f1f1f;
  padding-right: 25rpx;
  box-sizing: border-box;
  overflow: hidden;
}
.main_pay{
   width: 100%;
  height: 80rpx;
	background-color: #ffffff;
}
.main_pay .li_one{
  width: 154rpx;
	height: 64rpx;
	background-color: #ffffff;
	border-radius: 6rpx;
	border: solid 1px #cccccc;
  float: right;
  font-family: PingFang-SC-Medium;
	font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 64rpx;
	letter-spacing: 0px;
	color: #1f1f1f;
  text-align: center;
  margin-right: 30rpx;
}
.main_pay .li_two{
  width: 172rpx;
	height: 64rpx;
	background-color: #ec6d56;
	border-radius: 6rpx;
	border: solid 1px #ec6d56;
  float: right;
  font-family: PingFang-SC-Medium;
	font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 64rpx;
	letter-spacing: 0px;
	color: #ffffff;
  text-align: center;
  margin-right: 30rpx;
}

/* 支付弹窗 */

.payfor {
  width: 100%;
  height: 380rpx;
  background-color: white;
  position: fixed;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 100;
}

.payfor .pay_method {
  width: 100%;

  height: 100rpx;
  line-height: 100rpx;
  color: #1f1f1f;
  font-size: 32rpx;
  background-color: #f8f8f8;
  text-align: center;
}

.payfor .wechat_pay {
  width: 100%;
  height: 140rpx;
  color: #2a2a2a;
  line-height: 140rpx;
  font-size: 28rpx;
  text-align: center;
  overflow: hidden;
}

.payfor .wechat_pay image {
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}

.payfor .extra_pay {
  width: 100%;
  height: 140rpx;
  color: #2a2a2a;
  line-height: 140rpx;
  font-size: 28rpx;
  text-align: center;
  overflow: hidden;
  position: relative;
}

.payfor .extra_nopay {
  color: #ccc;
}

.payfor .extramoney {
  color: #ccc;
  font-size: 24rpx;
  position: absolute;
  left: 460rpx;
  top: 60rpx;
  width: auto;
  height: 30rpx;
  line-height: 30rpx;
}

.payfor .extra_pay image {
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}
.notorder{
  width: 100%;
  height: 300rpx;
  /* margin: 0 auto; */
