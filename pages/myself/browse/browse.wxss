/* pages/myself/browse/browse.wxss */
.container{
  width: 100%;
  height: auto;
  margin-bottom: 50rpx;
}
.main{
  width: 100%;
  height: auto;
}
.main .main_time{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  margin-left: 50rpx;
}
.main .main_ul{
  width: 750rpx;
	height: auto;
	background-color: #ffffff;
  border-bottom: 1px solid #f5f5f5;
}
.main .main_ul .f_ul{
  width: 750rpx;
	height: 210rpx;
	background-color: #ffffff;
  
}
.main .main_ul:last-child{
  border-bottom: none;
}
.main_ul .f_ul .select{
  width: 36rpx;
	height: 36rpx;
  float: right;
  margin-right: 25rpx;
  margin-top: 82rpx;
}
.main .main_ul .goods_img{
  width: 148rpx;
	height: 148rpx;
  float: left;
  margin-left: 24rpx;
  margin-top: 31rpx;
}
.main .main_ul .main_item{
  max-width: 550rpx;
  height: 100%;
  float: left;
}

.main .main_ul .main_item .item_one{
  max-width: 450rpx;
  height:105rpx;
  line-height: 105rpx;
  font-family: PingFang-SC-Medium;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #1f1f1f;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding-left: 20rpx;
}
.main .main_ul .main_item .item_two{
  max-width: 550rpx;
  height:105rpx;
  line-height: 105rpx;
  font-family: PingFang-SC-Medium;
	font-size: 36rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #ec6d56;
  padding-left: 20rpx;
}
/* 编辑和删除 */
.selectBtn{
  position: fixed;
  width: 80rpx;
  height: 120rpx;
  background-color: rgba(236, 109, 86, 0.8);
  /* background-color: black; */
  right: 24rpx;
  bottom: 80rpx;
}
.selectBtn .bianji{
  width: 80rpx;
	height: 80rpx;
  background-color: rgba(236, 109, 86, 0.8);
  position: absolute;
  top: 75rpx;
  border-radius: 100%;
}
.selectBtn .shanchu{
  width: 80rpx;
	height: 80rpx;
  background-color: rgba(236, 109, 86, 0.8);
  position: absolute;
  top: -40rpx;
  border-radius: 100%;
}
.main_pay{
   width: 100%;
  height: 88rpx;
	background-color: #ffffff;
  position: fixed;
  left: 0;
  bottom: 0;
  right: 0;
 
 
}
.main_pay .li_one{
  width: 154rpx;
	height: 64rpx;
	
	border-radius: 6rpx;
  background-color: #ec6d56;

  float: right;
  font-family: PingFang-SC-Medium;
	font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 64rpx;
	letter-spacing: 0px;
	color:white;
  text-align: center;
  margin-right:60rpx;
  margin-top: 12rpx;
}
.main_pay .li_two{
  width: 172rpx;
	height: 64rpx;
	background-color: #ffffff;
	border-radius: 6rpx;
	border: solid 1px #ec6d56;
  float: right;
  font-family: PingFang-SC-Medium;
	font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 64rpx;
	letter-spacing: 0px;
	color:#1f1f1f;
  text-align: center;
  margin-right: 60rpx;
   margin-top: 12rpx;
}