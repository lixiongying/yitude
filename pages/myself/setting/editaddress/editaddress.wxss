/* pages/myself/setting/goodsaddress/goodsaddress.wxss */
.container{
  width: 100%;
  height: auto;
}
.main{
  width: 750rpx;
	height: 412rpx;
	background-color: #ffffff;
  margin-bottom: 25rpx;
}
.main .input_item{
  width: 750rpx;
	height: 102rpx;
  border-bottom: 1px solid #f5f5f5;
}
.main .input_item input{
   width: 100%;
   height: 100%;
   font-family: PingFang-SC-Medium;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
   padding-left: 24rpx;
	letter-spacing: 0px;
	color: #1f1f1f;
}
.main  .select_ads .ads_title{
  float: left;
  width: 150rpx;
	height: 102rpx;
	font-family: PingFang-SC-Medium;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 102rpx;
	letter-spacing: 0px;
	color: #1f1f1f;
  margin-left: 24rpx;
}
.main  .select_ads .ads_where{
  max-width: 450rpx;
	height: 102rpx;
	font-family: PingFang-SC-Medium;
	font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 102rpx;
	letter-spacing: 0px;
	color: #666666;
  float: right;
  margin-right: 14rpx;
}
.main  .select_ads .ads_icon{
  width: 16rpx;
	height: 30rpx;
  float: right;
  margin-top: 36rpx;
  margin-right: 24rpx;
}
.save{
  width: 702rpx;
	height: 88rpx;
	background-color: #ec6d56;
	border-radius: 10rpx;
  margin-left: 24rpx;
  font-family: PingFang-SC-Medium;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 88rpx;
	letter-spacing: 0px;
	color: #ffffff;
  text-align: center;
  margin-top: 510rpx;
}