/* pages/myself/setting/goodsaddress/goodsaddress.wxss */
.container{
  width: 100%;
  height: auto;
  margin-bottom: 70rpx;
}
.bottom_title{
 
  width: 100%;
  height: 105rpx;
	color: #666666;
  line-height: 105rpx;
  text-align: center;
  font-size: 32rpx;

}
.main{
  width: 750rpx;
	height: 260rpx;
	background-color: #ffffff;
  margin-bottom: 25rpx;
}
.main .main_name{
  max-width: 625px;
	height: 40rpx;
	font-family: PingFang-SC-Medium;
	font-size: 36rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 40rpx;
	letter-spacing: 0px;
	color: #1f1f1f;
  margin-left: 24rpx;
  padding-top: 33rpx;

}
.main .main_ads{
  max-width: 625px;
	height: 40rpx;
	font-family: PingFang-SC-Medium;
	font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 40rpx;
	letter-spacing: 0px;
	color: #1f1f1f;
  margin-left: 24rpx;
  margin-top: 33rpx;
}
.main .main_caz{
  width: 100%;
  height: 125rpx;

}
.main .main_caz .select{
  width: 36rpx;
	height: 36rpx;
  float: left;
  margin-top: 45rpx;
  margin-left: 25rpx;
  float: left;
}
.main .main_caz .default{
  width: 136rpx;
	height: 40rpx;
  float: left;
  font-family: PingFang-SC-Medium;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 40rpx;
	letter-spacing: 0px;
	color: #666666;
  margin-top: 42.5rpx;
   margin-left: 25rpx;
}
.main .main_caz .edit,.delete{
  float: right;
  width: 100rpx;
	height: 48rpx;
	background-color: #ffffff;
	border-radius: 6rpx;
	border: solid 1px #cccccc;
  text-align: center;
  line-height: 48rpx;
  margin-top: 38.5rpx;
     margin-right: 24rpx;
}
.addads{
  width: 750rpx;
	height: 88rpx;
	background-color: #f8f8f8;
  position: fixed;
  bottom: 0;
  left: 0;
  line-height: 88rpx;
  text-align: center;
font-family: PingFang-SC-Medium;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #1f1f1f;
}