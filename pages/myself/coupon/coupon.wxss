.container{
  width: 100%;
  height: auto;
}
.nav{
  width: 100%;
  height: 80rpx;
	background-color: #ffffff;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
}
.nav .swiper-tab{
   width: 100%;
  height: 80rpx;
}
.nav .swiper-tab .on{
   color: #ec6d56;
}
.nav .swiper-tab-list{
  width: 33.3%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #1f1f1f;
  float: left;
}
.main_ul{
  width: 100%;
  height: auto;
}
.main_ul .main_li{
  width: 718rpx;
	height: 280rpx;
  margin-left: 16rpx;
  background: white;
  margin-top: 24rpx;
}
.main_ul .main_li .main_pic{
  width: 100%;
  height: 134rpx;
  background-color: #48e5ba;
}
.main_ul .main_li .main_pic .li_one{
  width: 20%;
  height: 100%;
  float: left;
  margin-left: 32rpx;
}
.main_pic .li_one view:first-child{
  width: 75%;
  height: 100%;
  line-height: 134rpx;
  font-family: Ebrima;
	font-size: 91rpx;
	font-weight: normal;
	font-stretch: normal;
  float: left;
	letter-spacing: -6rpx;
	color: #ffffff;
}
.main_pic .li_one view:last-child{
  width: 25%;
  height: 50rpx;
  line-height:50rpx;
  font-family: Ebrima;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 1rpx;
	color: #ffffff;
  float: left;
}
.main_ul .main_li .main_pic .li_two{
  width: 50%;
  height: 100%;
  float: left;
}
.main_ul .main_li .main_pic .li_two view:first-child{
  width: 100%;
  height: 57rpx;
  line-height: 57rpx;
  text-align: left;
  font-family: PingFang-SC-Medium;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 2rpx;
	color: #ffffff;
  margin-top: 10rpx;
}
.main_ul .main_li .main_pic .li_two view:last-child{
  width: 100%;
  height: 57rpx;
  line-height: 57rpx;
  font-family: PingFang-SC-Medium;
	font-size: 26rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 2rpx;
	color: #ffffff;
   /* margin-top: 10rpx; */
}
.main_ul .main_li .main_pic .li_three{
  width: 128rpx;
	height: 60rpx;
	background-color: #5831c5;
  margin-top: 37rpx;

	border-radius: 29rpx;
  float: right;
  font-family: PingFang-SC-Medium;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 60rpx;
	letter-spacing: 1px;
	color: #ffffff;
  text-align: center;
  margin-right: 35rpx;
}
.main_ul .main_li .main_pic .haveuse{
  background-color: #cccccc;
}
.main_ul .main_li .f_li{
  width: 100%;
  height: 146rpx;
}
.main_ul .main_li .f_li .li_item{
  width: 100%;
  height: 73rpx;
}
.f_li .li_item .timepic{
  float: left;
  width: 26rpx;
	height: 26rpx;
  margin-left: 32rpx;
  margin-top: 19rpx;
}
.f_li .li_item .times{
  float: left;
  max-width: 600rpx;
	height: 73rpx;
  line-height: 73rpx;
  text-align: left;
  margin-left: 20rpx;
  font-family: PingFang-SC-Medium;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 1rpx;
	color: #666666;
}

.coupon_box_li {
  width: 700rpx;
  margin: 0 auto 25rpx;
  background: #ffffff;
  position: relative;
  height: 270rpx;
  box-shadow: 0px 2rpx 8rpx 0px 
		rgba(204, 204, 204, 0.75);
}
.coupon_box_li_main {
  position: absolute;
  width: 100%;
  height: 270rpx;
  left: 0;
  top: 0;
}
.coupon_box_li_top_bg {
  width: 100%;
  position: absolute;
  height: 100%;
  left: 0;
  top: 0;
}
.coupon_box_li_top_bg image {
  width: 100%;
  height: 100%;
}
.coupon_box_li_top {
  width: 100%;
  height: 132rpx;
  position: relative;
}
.coupon_box_li_top_con_l {
  float: left;
  width: 210rpx;
  height: 132rpx;
}
.coupon_box_li_top_con_l_num {
  font-size: 92rpx;
  color: #ffffff;
  position: relative;
  letter-spacing: -6rpx;
  float: left;
  padding-left: 30rpx;
  height: 132rpx;
  line-height: 132rpx;

}
.coupon_box_li_top_con_l_num text {
  position: absolute;
  width: 20rpx;
  height: 25rpx;
  line-height: 25rpx;
  top: 20rpx;
  right: -25rpx;
  font-size: 32rpx;
  color:#ffffff;
}
.coupon_box_li_top_con_c {
  float: left;
  width: 300rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  flex-direction: column;
}
.coupon_box_li_top_con_c_tit {
  height: 48rpx;
  font-size: 32rpx;
  color: #ffffff;
  letter-spacing: 2rpx;
}
.coupon_box_li_top_con_c_msg {
  height: 48rpx;
  font-size: 26rpx;
  color: #ffffff;
  letter-spacing: 2rpx;
}
.share_btn {
  position: absolute;
  width: 130rpx;
	height: 60rpx;
	background-color: #5831c5;
	box-shadow: 0px 8px 12px 0px 
		rgba(104, 65, 211, 0.5);
	border-radius: 30rpx;
  top: 20rpx;
  right: 25rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 24rpx;
  color: #ffffff;
}
.share_btn_use{
  background-color: #cccccc;
	box-shadow: 0px 8px 12px 0px 
		rgba(102, 102, 102, 0.5);
}

.coupon_box_li_con {
  width: 100%;
  height: 148rpx;
  padding-top: 16rpx;
  box-sizing: border-box;
  padding-left: 25rpx;
}
.coupon_box_li_con_li {
  display: flex;
  align-items: center;
  height: 50rpx;
  font-size: 24rpx;
  color: #666666;
}
.coupon_box_li_con_li image {
  width: 26rpx;
  height: 26rpx;
  margin-right: 15rpx;
}