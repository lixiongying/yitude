/* pages/myself/myassets/mybankcard/mybankcard.wxss */
.container{
  width: 100%;
  height: auto;
}
.card{
  width: 702rpx;
	height: 148rpx;
	background-color: #ffffff;
	border-radius: 10rpx;
  margin-left: 24rpx;
  margin-top: 24rpx;
}
.card_img{
  width: 44rpx;
	height: 44rpx;
  float: left;
  margin-left: 24rpx;
  margin-top: 24rpx;
}
.card_content{
  width: 50%;
  height: 148rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  flex-direction: column;
  float: left;
}
.bankname{
  font-family: PingFang-SC-Medium;
	font-size: 32rpx;
	color: #333333;
}
.banknumber{
  font-size: 28rpx;
}
.card_next{
  width: 17rpx;
	height: 29rpx;
  float: right;
  margin-top: 60rpx;
  margin-right: 24rpx;
}
.addcard{
  width: 100%;
  height: 88rpx;
	background-color: #f8f8f8;
  text-align: center;
  line-height: 88rpx;
  font-family: PingFang-SC-Medium;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #1f1f1f;
  position: fixed;
  bottom: 0;
  left: 0;
}