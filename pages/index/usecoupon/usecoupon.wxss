.container{
  width: 100%;
  height: auto;
}
.main_ul{
  width: 100%;
  height: auto;
}
.main_ul .main_li{
  width: 718rpx;
	height: 280rpx;
  margin-left: 16rpx;
  background: white;
  margin-top: 24rpx;
}
.main_ul .main_li .main_pic{
  width: 100%;
  height: 134rpx;
  background-image: url(data:image/png;base64,);

  background-repeat:no-repeat; 
  background-size:100% 100%;
  /* background-color: #48e5ba; */
}
.main_ul .main_li .main_pic .li_one{
  width: 20%;
  height: 100%;
  float: left;
  margin-left: 32rpx;
}
.main_pic .li_one view:first-child{
  width: 75%;
  height: 100%;
  line-height: 134rpx;
  font-family: Ebrima;
	font-size: 91rpx;
	font-weight: normal;
	font-stretch: normal;
  float: left;
	letter-spacing: -6rpx;
	color: #ffffff;
}
.main_pic .li_one view:last-child{
  width: 25%;
  height: 50rpx;
  line-height:50rpx;
  font-family: Ebrima;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 1rpx;
	color: #ffffff;
  float: left;
}
.main_ul .main_li .main_pic .li_two{
  width: 50%;
  height: 100%;
  float: left;
}
.main_ul .main_li .main_pic .li_two view:first-child{
  width: 100%;
  height: 57rpx;
  line-height: 57rpx;
  text-align: left;
  font-family: PingFang-SC-Medium;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 2rpx;
	color: #ffffff;
  margin-top: 10rpx;
}
.main_ul .main_li .main_pic .li_two view:last-child{
  width: 100%;
  height: 57rpx;
  line-height: 57rpx;
  font-family: PingFang-SC-Medium;
	font-size: 26rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 2rpx;
	color: #ffffff;
   /* margin-top: 10rpx; */
}
.main_ul .main_li .main_pic .li_three{
  width: 128rpx;
	height: 60rpx;
	background-color: #5831c5;
  margin-top: 37rpx;

	border-radius: 29rpx;
  float: right;
  font-family: PingFang-SC-Medium;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 60rpx;
	letter-spacing: 1px;
	color: #ffffff;
  text-align: center;
  margin-right: 35rpx;
}
.main_ul .main_li .main_pic .haveuse{
  background-color: #cccccc;
}
.main_ul .main_li .f_li{
  width: 100%;
  height: 146rpx;
}
.main_ul .main_li .f_li .li_item{
  width: 100%;
  height: 73rpx;
}
.f_li .li_item .timepic{
  float: left;
  width: 26rpx;
	height: 26rpx;
  margin-left: 32rpx;
  margin-top: 19rpx;
}
.f_li .li_item .times{
  float: left;
  max-width: 600rpx;
	height: 73rpx;
  line-height: 73rpx;
  text-align: left;
  margin-left: 20rpx;
  font-family: PingFang-SC-Medium;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 1rpx;
	color: #666666;
}