/**index.wxss**/
.container{
  width: 100%;
  height: auto;
  margin-bottom: 50rpx;
}
/* 头部搜索开始 */
.index_header{
  width: 750rpx;
	height: 80rpx;
	background-color: #ffffff;
  margin-top: 10rpx;
}
.index_header .posistion{
  width: 24rpx;
	height: 28rpx;
  float: left;
  margin-left: 24rpx;
  margin-top: 27rpx;
}
.index_header .posistion image{
  width: 22rpx;
	height: 26rpx;
  
}
.index_header .city{
  width: 100rpx;
	height: 80rpx;
	font-family: PingFang-SC-Medium;
	font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 80rpx;
	letter-spacing: 0px;
	color: #1f1f1f;
  float: left;
  margin-left: 12rpx;
}
.index_header .serach{
  width: 452rpx;
	height: 60rpx;
	background-color: rgba(240, 240, 240, 0.75);
	border-radius: 29rpx;
  float: left;
  margin-top: 10rpx;
  position: relative;
}
.serach_icon{
  position: absolute;
  width: 35rpx;
	height: 35rpx;
  top: 15rpx;
  left: 300rpx;
}
.index_header .serach .serach_input{
  width: 100%;
  height:100%;
  text-align: center;

	background-color: rgba(240, 240, 240, 0.75);
	border-radius: 29rpx;

}
.index_header .comment{
  width: 44rpx;
	height: 40rpx;
  float: right;
  margin-right: 24rpx;
  margin-top: 22rpx;
  position: relative;
}
.index_header .comment image{
  width: 44rpx;
	height: 40rpx;
}
.circle{
  position: absolute;
  width: 15rpx;
  height: 15rpx;
  background-color: red;
  border-radius: 100%;
  top: 0;
  right: 0;
}
/* 头部搜索结束 */


/* 轮播开始 */
.index_slider{
  width: 100%;
 height: 400rpx; 
}
.index_slider swiper{
  width: 100%;
 height: 400rpx; 
}
.index_slider swiper-item{
  width: 100%;
 height: 400rpx; 
}
.index_slider swiper-item image{
  width: 100%;
 height: 400rpx; 
}
/* 轮播结束 */


/* 公告开始 */
.index_anounce{
  width: 750rpx;
	height: 64rpx;
	background-color: #fff5f0
}
.index_anounce .anounce_one{
  width: 28rpx;
	height: 28rpx;
  float: left;
  /* margin-left: 24rpx;
  margin-top: 18rpx; */
}
.index_anounce image{
  display: block;
  width: 28rpx;
	height: 28rpx;
   float: left;
  margin-left: 24rpx;
  margin-top: 18rpx; 
}
.index_anounce .anounce_two{
  max-width: 600rpx;
  height: 64rpx;
  line-height: 64rpx;
  padding-left: 20rpx;
  float: left;
}
/* 公告结束 */

/* 分类开始 */
.index_classify{
  width: 100%;
  height: 200rpx;
  background-color: #ffffff;
  overflow: auto;
  white-space: nowrap;
  -webkit-overflow-scrolling: touch;
  text-align: center;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.index_classify .main_lis{
  display: inline-block;
  width: 25%;
  height: 200rpx;
}
.main_lis image{
  width: 92rpx;
	height: 92rpx;
  margin: 0 auto;
  margin-top: 20rpx;
}
.main_lis view{
 font-family: PingFang-SC-Medium;
	font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #1f1f1f;
  margin-bottom: 20rpx;
  width: 100%;
  text-align: center;
  line-height: 40rpx;
  height: 40rpx;
   margin-top: 10rpx;
}
/* 分类结束*/

/* 热销商品开始 */
.index_goods{
  width: 750rpx;
  height: auto;
	background-color: #ffffff;
  margin-top: 20rpx;
}
.index_goods .goodshead{
  width: 750rpx;
	height: 102rpx;
	background-color: #ffffff;
}
.index_goods .goodshead .li_one{
  width:200rpx;
	height: 102rpx;
	font-family: PingFang-SC-Bold;
	font-size: 36rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 102rpx;
	letter-spacing: 0px;
	color: #1f1f1f;
  margin-left: 24rpx;
  float: left;
}
.index_goods .goodshead .li_two{
  width:202rpx;
	height: 102rpx;
	font-family: PingFang-SC-Bold;
	font-size: 26rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 102rpx;
	text-align: right;
  padding-right: 20rpx;
	color: #1f1f1f;
  margin-left: 24rpx;
  float:right;
}
.index_goods .goodshead .li_three{
  width: 15rpx;
	height: 25rpx;
  float: right;
  margin-top: 40rpx;
  margin-right: 30rpx;

}
.index_goods .goodshead .li_three image{
  width: 15rpx;
	height: 25rpx;
}
.index_goods scroll-view{
  width: 100%；
  height：280rpx;
  white-space: nowrap;
}
.index_goods .head_item{
  width: 280rpx;
	height: 482rpx;
  /* float: left; */
  display: inline-block; 
  margin-left: 24rpx;
  margin-bottom: 20rpx;
}
.index_goods .head_item .head_img{
  width: 280rpx;
	height: 280rpx;
}
.index_goods .head_item .f_li{
 width: 100%;
  height: 60rpx;
  margin-top: 10rpx;
}

.index_goods .biaoqian .qianone{
  float: left;
  width: auto;
	height: 40rpx;
  padding: 0 10rpx;
	border-radius: 10rpx;
  line-height: 40rpx;
  font-size: 22rpx;
  text-align: center;
  margin-top: 10rpx;
  margin-right: 20rpx;
}
.index_goods .biaoqian .qianone{
  background-color: #fff5f8;
  color: #a74e68;
  /* margin-left: 22rpx; */

}
.head_item .name{
  width: 100%;
	height: 51rpx;
	font-family: PingFang-SC-Medium;
	font-size: 28rpx;
	line-height: 51rpx;
	letter-spacing: 0px;
	color: #1f1f1f;
  padding-left: 14rpx;
}
.head_item .price{
  width: 100%;
	height: 51rpx;
	font-family: PingFang-SC-Medium;
	font-size: 32rpx;
	line-height: 51rpx;
	letter-spacing: 0px;
	color: #ec6d56;
  padding-left: 13rpx;
}
/* 精品推荐 */
.main .main_ul{
   width: 100%;
   height: auto;
   background: white;
   display: flex;
   justify-content: flex-start;
   align-items: center;
   flex-wrap: wrap;
   padding-top: 24rpx;
}
.main_ul .main_li{
  width: 340rpx;
	height: 520rpx;
  margin-left: 24rpx;
  margin-bottom: 20rpx;
}

.main_li .goodsimg{
  width: 340rpx;
	height: 340rpx;
}
.main_li .goodsname{
  width: 100%;
  height: 60rpx;
  font-family: PingFang-SC-Medium;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 60rpx;
	letter-spacing: 0px;
	color: #1f1f1f;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.main_li .biaoqian{
  width: 100%;
  height: 60rpx;
}
.main_li .biaoqian .qianone,.qiantwo{
  float: left;
  width: auto;
	height: 40rpx;
  padding: 0 10rpx;
	border-radius: 10rpx;
  line-height: 40rpx;
  font-size: 22rpx;
  text-align: center;
  margin-top: 10rpx;
  margin-right: 20rpx;
}
.main_li .biaoqian .qianone{
  background-color: #fff5f8;
  color: #a74e68;
  /* margin-left: 22rpx; */

}
.main_li .biaoqian .qiantwo{
  background-color: #fffcf5;
  color: #ecc356;
}
.main_li .pandc{
  width: 100%;
  height: 60rpx;
}
.main_li .pandc .price{
  width: 50%;
  height: 60rpx;
  font-family: PingFang-SC-Medium;
	font-size: 32rpx;
  text-align: left;
  line-height: 60rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: -1px;
	color: #ec6d56;
  float: left;
}
.main_li .pandc .comment{
  width: 50%;
  height: 60rpx;
  font-family: PingFang-SC-Medium;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 60rpx;
	letter-spacing: 0px;
	color: #cccccc;
  float: right;
  text-align: right;
}

/* 分享领取优惠券弹窗 */
.getcoupon{
   position: fixed;
  width: 702rpx;
	height: 840rpx;
	background-color: #ffffff;
	border-radius: 10rpx;
  top: 50%;
  margin-top: -420rpx;
  left: 50%;
  margin-left: -351rpx;
  z-index: 100;
}

.getcoupon .co_one{
  width: 702rpx;
	height: 250rpx;
  font-family: PingFang-SC-Bold;
	font-size: 50rpx;
	line-height:250rpx;
  text-align: center;
	letter-spacing: 2px;
	color: #6c45d5;
  position: relative;
}
.getcoupon .co_one image{
  position: absolute;
  width: 100%;
  height: 100%;
