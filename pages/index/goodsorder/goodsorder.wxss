/* pages/index/orderPay/index.wxss */

.container {
  padding-bottom: 91rpx;
  background: #f0f0f0;
}

.order_top {
  width: 100%;
  height: 146rpx;
  position: relative;
  background: #fff;
}

.goodDetails {
  border-top: 1px solid #f0f0f0;
}

.order_top_icon {
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  height: 4rpx;
}

.order_top .ads_btnnext {
  position: absolute;
  top: 56rpx;
  right: 25rpx;
  width: 17rpx;
  height: 29rpx;
}

.order_top_true .person_msg {
  width: 100%;
  height: 73rpx;
  font-size: 32rpx;
  line-height: 73rpx;
  letter-spacing: 0px;
  color: #1f1f1f;
  margin-left: 24rpx;
}

.order_top_true .person_ads {
  width: 100%;
  height: 73rpx;
}

.order_top_true .person_ads .ads_btn {
  width: 72rpx;
  height: 32rpx;
  border-radius: 15rpx;
  border: solid 1px #ec6d56;
  font-size: 22rpx;
  color: #ec6d56;
  line-height: 32rpx;
  text-align: center;
  margin-left: 24rpx;
  margin-top: 10rpx;
  float: left;
}

.order_top_true .person_ads .ads_address {
  max-width: 516rpx;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 28rpx;
  color: #1f1f1f;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  float: left;
  margin-left: 24rpx;
  margin-top: 10rpx;
}

.order_top_flase {
  width: 100%;
  height: 100%;
  position: relative;
  padding-top: 1rpx;
}

.order_top_flase_msg {
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  line-height: 40rpx;
  margin-top: 10rpx;
  color: #ccc;
}

.order_top_flase_btn {
  width: 240rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 28rpx;
  margin: 0 auto;
  border: 1px solid #ccc;
  margin-top: 10rpx;
  border-radius: 10rpx;
}

.main_ul {
  width: 750rpx;
  height: auto;
  background-color: #fff7f5;
}

.main_ul .f_ul {
  width: 750rpx;
  height: 210rpx;
}

.main_ul .f_ul .number {
  width: 60rpx;
  height: 36rpx;
  float: right;
  margin-right: 25rpx;
  margin-top: 135rpx;
}

.main_ul .goods_img {
  width: 148rpx;
  height: 148rpx;
  float: left;
  margin-left: 24rpx;
  margin-top: 31rpx;
}

.main_ul .main_item {
  width: auto;
  height: 100%;
  margin-left: 20rpx;
  float: left;
}

.main_ul .main_item .item_one {
  width: 450rpx;
  height: 105rpx;
  line-height: 105rpx;
  font-family: PingFang-SC-Medium;
  font-size: 32rpx;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #1f1f1f;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.main_ul .main_item .item_two {
  max-width: 450rpx;
  height: 105rpx;
  line-height: 105rpx;
  font-family: PingFang-SC-Medium;
  font-size: 36rpx;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #ec6d56;
}

.main_price {
  width: 100%;
  height: 80rpx;
  background-color: #fff;
  text-align: right;
  font-family: PingFang-SC-Medium;
  font-size: 26rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 80rpx;
  letter-spacing: 0px;
  color: #1f1f1f;
  padding-right: 25rpx;
}

.main_head {
  width: 750rpx;
  height: 80rpx;
  background-color: #fff;
  margin-top: 20rpx;
}

.main_head .head_img {
  width: 48rpx;
  height: 48rpx;
  float: left;
  margin-left: 24rpx;
  margin-top: 16rpx;
}

.head_title {
  max-width: 600rpx;
  height: 80rpx;
  text-align: left;
  padding-left: 24rpx;
  line-height: 80rpx;
  font-size: 26rpx;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #1f1f1f;
  float: left;
}

.main_content {
  width: 750rpx;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 28rpx;
  color: #1f1f1f;
  background-color: #fff;
  border-bottom: 1px solid #f5f5f5;
}

.cot_one {
  float: left;
  margin-left: 24rpx;
}

.cot_two {
  float: right;
  margin-right: 24rpx;
}

.cot_btn {
  width: 14rpx;
  height: 25rpx;
  float: right;
  margin-right: 24rpx;
  margin-top: 32rpx;
}

.use_coin {
  float: right;
  margin-right: 24rpx;
  width: 14rpx;
  height: 25rpx;
  margin-top: 37rpx;
}

.use_coins {
  float: right;
  margin-right: 24rpx;
  width: 25rpx;
  height: 14rpx;
  margin-top: 37rpx;
}

.coin {
  width: 100%;
  height: 102rpx;
  background-color: #fff;
}

.coin .coin_one {
  max-width: 500rpx;
  height: 102rpx;
  float: left;
  line-height: 102rpx;
}

.jinbi {
  color: #666;
  float: left;
  margin-left: 24rpx;
}

.usejinbi {
  color: #ecc356;
  float: left;
  margin-left: 24rpx;
}

.coin_amount {
  width: 160rpx;
  height: 54rpx;
  background-color: #fff;
  border-radius: 6rpx;
  text-align: center;
  color: #1f1f1f;
  font-size: 26rpx;
  border: solid 1px #ccc;
  float: left;
  margin-top: 27rpx;
}

.coin_two {
  width: auto;
  height: 102rpx;
  font-size: 24rpx;
  letter-spacing: 0px;
  color: #ccc;
  float: right;
  margin-right: 24rpx;
  line-height: 102rpx;
}

.main_info {
  width: 750rpx;
  height: auto;
  background-color: #fff;
  margin-top: 24rpx;
}

.main_info .infomsg {
  padding: 24rpx;
}

.main_info .infomsg view {
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  text-align: right;
  font-size: 24rpx;
  color: #1f1f1f;
  padding: 5rpx 0;
}

.submit_info {
  width: 100%;
  height: 88rpx;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
}

.submit_info .sub_mssage {
  max-width: 503px;
  height: 88rpx;
  font-size: 24rpx;
  line-height: 88rpx;
  letter-spacing: 0px;
  margin-left: 24rpx;
  float: left;
}

.submit_info .sub_btn {
  width: 300rpx;
  height: 88rpx;
  background-color: #4f4765;
  font-family: PingFang-SC-Medium;
  font-size: 32rpx;
  line-height: 88rpx;
  color: #fff;
  text-align: center;
  float: right;
}

/* 支付弹窗 */

.payfor {
  width: 100%;
  height: 380rpx;
  background-color: white;
  position: fixed;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 100;
}

.payfor .pay_method {
  width: 100%;
  height: 100px;
  height: 100rpx;
  line-height: 100rpx;
  color: #1f1f1f;
  font-size: 32rpx;
  background-color: #f8f8f8;
  text-align: center;
}

.payfor .wechat_pay {
  width: 100%;
  height: 140rpx;
  color: #2a2a2a;
  line-height: 140rpx;
  font-size: 28rpx;
  text-align: center;
  overflow: hidden;
}

.payfor .wechat_pay image {
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}

.payfor .extra_pay {
  width: 100%;
  height: 140rpx;
  color: #2a2a2a;
  line-height: 140rpx;
  font-size: 28rpx;
  text-align: center;
  overflow: hidden;
  position: relative;
}

.payfor .extra_nopay {
  color: #ccc;
}

.payfor .extramoney {
  color: #ccc;
  font-size: 24rpx;
  position: absolute;
  left: 460rpx;
  top: 60rpx;
  width: auto;
  height: 30rpx;
  line-height: 30rpx;
}

.payfor .extra_pay image {
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}

.payPass_box1 {
  font-size: 26rpx;
  color: #333;
  width: 640rpx;
  text-align: center;
  margin-top: 40rpx;
  height: 400rpx;
  position: fixed;
  left: 50%;
  top: 30%;
  transform: translate(-50%, -50%);
  border: 2rpx #d1d1d1 solid;
  background: #fff;
  opacity: 0.9;
}

.payPass_box_t {
  margin-top: 100rpx;
}

.payPass_box_m {
  margin-top: 80rpx;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
}

.payPass_box_m_in {
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  z-index: 10;
}

.payPass_box_m_in_in {
  width: 70rpx;
  height: 70rpx;
  border: 4rpx #bfbfbf solid;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 8rpx;
}

.indexTopInput {
  position: absolute;
  height: 100%;
  background: #000;
  opacity: 0.5;
  z-index: 2;
  /* left: 0rpx; */
  background: red;
  left: -150%;
}

.indexTopInput input {
  width: 100%;
  height: 100%;
}

.qiueBtn {
  position: absolute;
  right: 20rpx;
  top: 10rpx;
  width: 80rpx;
  height: 60rpx;
  text-align: right;
  color: #999;
}
