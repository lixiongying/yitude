/* pages/index/classify/classify.wxss */
.container{
  width: 100%;
  background: white;

}
.intel_left{
  width: 175rpx;
	height:1200rpx;
	/* background-color: #f0eff4; */
  float: left;
}
.intel_left .left_li{
  width: 175rpx;
  height: 98rpx;
  font-family: PingFang-SC-Medium;
	font-size: 26rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 98rpx;
	letter-spacing: 1rpx;
  text-align: center;
	color: #b3b3b3;
}
.intel_left .active{
  background-color: #ffffff;
  color: #ec6d56;
}

.intel_right{
    width: 575rpx;
    float: right;
   
   	background-color: white;
}
.intel_right .main_ul{
    width: 575rpx;
    height: auto;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    flex-wrap:wrap;
} 
.intel_right .main_ul .main_li{
    width: 33.3%; 
    height: 186rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-around;
   
} 
.intel_right .main_ul .main_li image{
   width: 136rpx;
	 height: 135rpx;
   border-radius: 100%;
} 
